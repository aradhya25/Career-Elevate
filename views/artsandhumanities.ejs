        <!-- Footer -->
        <footer>
            <div class="footer-content">
                <div class="footer-logo">
                    <svg xmlns="http://www.w3.org/2000/svg" width="32" height="32" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><path d="M12 2v4"/><path d="M8.5 8.5 12 12l-5 5"/><path d="M12 22v-4"/><path d="m15.5 15.5-3-3 5-5"/><path d="M2 12h4"/><path d="m22 12-4 4"/></svg>
                    <span>Career Elevate</span>
